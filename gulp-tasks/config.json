{
	"folderSettings": {
		"src": "frontend/src",
		"dist": "frontend/build",
		"images": "images",
		"scripts": "scripts",
		"styles": "styles",
		"vendor": "vendor",
		"bowerComponents": "frontend/libraries"
	},
	"filenameSettings": {
		"concatVendorJsFile": "vendor.js",
		"concatSrcJsFile": "src.js",
		"concatVendorCSSFile": "vendor.css",
		"concatSrcCSSFile": "src.css"
	},
	"browserSettings": {
		"supportedBrowsers": [
		    "ie >= 7",
		    "ie_mob >= 10",
		    "ff >= 30",
		    "chrome >= 34",
		    "safari >= 7",
		    "opera >= 23",
		    "ios >= 7",
		    "android >= 4.4",
		    "bb >= 10"
		]
	}, 
	"fileTypes": {
		"pages": ["html"],
		"fonts": ["eot", "svg", "ttf", "woff"],
		"images": ["jpg", "JPG", "png", "PNG"],
		"scripts": ["js"],
		"styles": ["css", "scss"]
	}
}